{
  "name": "iroa-dashboard",
  "version": "1.0.0",
  "description": "IROA Dashboard Frontend",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "build-css": "npx tailwindcss -i ./styles/input.css -o ./styles/output.css",
    "build-css-watch": "npx tailwindcss -i ./styles/input.css -o ./styles/output.css --watch",
    "update": "npm run build-css && npm run start"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "vite": "^7.0.6"
  }
}
